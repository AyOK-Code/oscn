<h1>New errors with parsing expected HTML</h1>
<% if @errors.any?{|x| x[:differences]}%>
<h2>Differences Key</h2>
<p>
  "-" = missing from expected<br/>
  "+" = new from expected<br/>
  "~" = different from expected<br/>
</p>
<% end %>
<h2>Errors</h2>
<% @errors.each do |error| %>
  <h3>Section: <%= error[:section] %></h3>
  <p>
    <%= error[:message] %><br/>
    <% if error[:differences] %>
      Differences:<br/>
      <% error[:differences].each do |difference| %>
        <%= difference %><br/>
      <% end %>
    <% end %>
  </p>
<% end %>
